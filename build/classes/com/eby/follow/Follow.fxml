<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import de.jensd.fx.glyphs.fontawesome.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" fx:id="paneParent" prefHeight="62.0" prefWidth="80.0" styleClass="header" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.eby.follow.FollowController">
   <children>
      <Button fx:id="btFacebook" mnemonicParsing="false" onAction="#facebookAction" prefHeight="30.0" prefWidth="30.0">
         <styleClass>
            <String fx:value="radius" />
            <String fx:value="facebook-color" />
         </styleClass>
      </Button>
      <Button fx:id="btGithub" layoutY="31.0" mnemonicParsing="false" onAction="#githubIcon" prefHeight="30.0" prefWidth="30.0">
         <styleClass>
            <String fx:value="radius" />
            <String fx:value="github-color" />
         </styleClass>
      </Button>
      <FontAwesomeIconView fx:id="fIcon" fill="WHITE" glyphName="FACEBOOK" layoutX="11.0" layoutY="19.0" size="13" />
      <FontAwesomeIconView fx:id="gIcon" fill="WHITE" glyphName="GITHUB" layoutX="9.0" layoutY="50.0" size="13" />
      <FontAwesomeIconView fx:id="hideIcon" fill="WHITE" glyphName="CHEVRON_LEFT" layoutX="40.0" layoutY="35.0" onMouseClicked="#hideAction" size="13" />
   </children>
   <stylesheets>
      <URL value="@../resources/css/MetroUIGlobal.css" />
      <URL value="@../resources/css/InsertIcon.css" />
   </stylesheets>
</AnchorPane>
